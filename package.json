{
  "name": "cute-emojies-confirm",
  "version": "1.0.0",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "author": "Masoud Rezakhanlou <masoud.rk95@gmail.com>",
  "license": "MIT",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn css:dev",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:styles": "rollup -c rollup.config.mjs  --debug",
    "css:prod": "sass --style compressed src/index.scss > lib/dist/index.min.css",
    "css:dev": "sass --style expanded src/index.scss > lib/dist/index.css",
    "dev": "chokidar 'src/**/*' -c 'yarn build'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "chokidar-cli": "^3.0.0",
    "postcss": "^8.4.47",
    "postcss-modules": "^6.0.0",
    "rollup": "^4.24.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.80.3",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
