{
  "name": "cute-emojies-confirm",
  "version": "1.0.0",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "author": "Masoud Rezakhanlou <masoud.rk95@gmail.com>",
  "license": "MIT",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn css:dev",
    "build:dev": "yarn build:esm && yarn css:dev",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "css:prod": "sass --style compressed src/index.scss > lib/dist/index.min.css",
    "css:dev": "sass --style expanded src/index.scss > lib/dist/index.css",
    "dev": "nodemon --watch src --ext ts,tsx,scss --exec \"yarn build:dev\""
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "postcss-modules": "^6.0.0",
    "sass": "^1.80.3",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.5"
  }
}
